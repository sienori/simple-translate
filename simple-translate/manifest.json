{

    "manifest_version": 2,
    "name": "Simple Translate",
    "version": "1.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["simple-translate.js"]
        }
    ]

}